{
  "name": "dogokit-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "analyze": "pnpm clean && dpdm **/*.*",
    "build": "remix build",
    "build:sentry": "remix build --sourcemap && sentry-upload-sourcemaps --quiet --org allnimal --project dogokit-remix",
    "check": "pnpm /^check:.*/",
    "check:data": "tsx prisma/utils/check-data.ts",
    "check:env": "tsx prisma/utils/check-env.ts",
    "check:eslint": "eslint --cache .",
    "check:prettier": "prettier --cache --check '**/*.{js,mjs,cjs,ts,tsx,json,md,mdx}'",
    "check:prisma": "prisma validate",
    "check:stylelint": "stylelint --cache ./app/styles/*.css",
    "check:ts-coverage": "typescript-coverage-report",
    "check:tsc": "tsc --noEmit",
    "clean": "rm -rf .cache build public/build",
    "db:debug": "prisma debug",
    "db:docs": "prisma-docs-generator serve",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:reset": "prisma db push --force-reset",
    "db:seed": "prisma db seed && pnpm check:data",
    "db:studio": "prisma studio --browser none",
    "debug:eslint": "DEBUG=eslint:cli-engine pnpm check:eslint",
    "dev": "remix dev --manual",
    "fix": "pnpm /^fix:.*/",
    "fix:eslint": "eslint --cache --fix .",
    "fix:prettier": "prettier --cache --write '**/*.{js,mjs,cjs,ts,tsx,json,md,mdx}'",
    "fix:prisma": "prisma format && prisma validate",
    "fix:stylelint": "stylelint --cache --fix ./app/styles/*.css",
    "postinstall": "prisma generate",
    "knip": "knip",
    "prod:check:data": "dotenv -e .env.production tsx prisma/utils/check-data.ts",
    "prod:check:env": "dotenv -e .env.production tsx prisma/utils/check-env.ts",
    "prod:db:migrate": "dotenv -e .env.production prisma migrate dev",
    "prod:db:push": "dotenv -e .env.production prisma db push",
    "prod:db:reset": "dotenv -e .env.production prisma db push --force-reset",
    "prod:db:seed": "dotenv -e .env.production prisma db seed && pnpm prod:check:data",
    "prod:db:studio": "dotenv -e .env.production prisma studio --browser none",
    "prod:dev": "dotenv -e .env.production remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "upgrade": "taze -w && taze major -w"
  },
  "dependencies": {
    "@conform-to/react": "^0.9.1",
    "@conform-to/zod": "^0.9.1",
